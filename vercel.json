{
  "version": 2,
  "name": "primeestate",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "landing.html", "use": "@vercel/static" },
    { "src": "properties.html", "use": "@vercel/static" },
    { "src": "property.html", "use": "@vercel/static" },
    { "src": "about.html", "use": "@vercel/static" },
    { "src": "contact.html", "use": "@vercel/static" },
    { "src": "dashboard.html", "use": "@vercel/static" },
    { "src": "styles/**/*", "use": "@vercel/static" },
    { "src": "scripts/**/*", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/landing", "destination": "/landing.html" },
    { "source": "/properties", "destination": "/properties.html" },
    { "source": "/property", "destination": "/property.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=600" }
      ]
    },
    {
      "source": "/(styles|scripts)/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}


